    <fieldset>
        <field  id="room"
                name="room"
                type="sql"
                label="COM_THM_ORGANIZER_RS_ROOM_TITLE"
                description="COM_THM_ORGANIZER_RS_ROOM_DESC"
                class="inputbox"
                query="SELECT id, longname FROM #__thm_organizer_rooms"
                key_field="id"
                value_field="longname" />
        <field  id="date"
                name="date"
                type="calendar"
                label="COM_THM_ORGANIZER_RS_DATE_TITLE"
                description="COM_THM_ORGANIZER_RS_DATE_DESC"
                class="inputbox validate-germandate"
                validate="germandate"
                format="%d.%m.%Y"
                size="7" />
    </fieldset>
</form>
